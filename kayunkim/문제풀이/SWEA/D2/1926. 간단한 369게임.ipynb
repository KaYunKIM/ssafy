    "T = int(input())\n",
    "bin = []\n",
    "for i in range(1,T+1):\n",
    "    bin.append(i)\n",
    "    cnt = 0\n",
    "    for j in range(len(str(i))):\n",
    "        if str(i)[j] == '3' or str(i)[j] == '6' or str(i)[j] == '9':\n",
    "            cnt+=1\n",
    "            bin[i-1] = '-'*cnt\n",
    "print(' '.join(map(str,bin)))"
   ]
  }
