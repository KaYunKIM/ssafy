    "T = int(input())\n",
    "for tc in range(1,T+1):\n",
    "    h1,m1,h2,m2 = list(map(int,input().split()))\n",
    "    H = h1+h2\n",
    "    M = m1+m2\n",
    "    if M >= 60:\n",
    "        H+=1\n",
    "        M-=60\n",
    "    if H >= 13:\n",
    "        H-=12\n",
    "    print('#{} {} {}'.format(tc,H,M))"
   ]
  }
