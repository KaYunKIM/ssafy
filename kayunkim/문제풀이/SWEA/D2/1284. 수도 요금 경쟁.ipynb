   "source": [
    "T = int(input())\n",
    "for tc in range(1,T+1):\n",
    "    P,Q,R,S,W = list(map(int,input().split()))\n",
    "    A = P*W\n",
    "    if W <= R:\n",
    "        B = Q\n",
    "    else:\n",
    "        B = Q+(W-R)*S\n",
    "    if A>=B:\n",
    "        print('#{} {}'.format(tc,B))\n",
    "    else:\n",
    "        print('#{} {}'.format(tc,A))"
   ]
  }
