   "source": [
    "T = int(input())\n",
    "for tc in range(1,T+1):\n",
    "    N = int(input())\n",
    "    bin = []\n",
    "    j = 1\n",
    "    while len(bin)!= 10:\n",
    "        a= str(N*j)\n",
    "        for i in a:\n",
    "            if i not in bin:\n",
    "                bin.append(i)\n",
    "        j+=1\n",
    "    print('#{} {}'.format(tc,(j-1)*N))"
   ]
  }
