    "T = int(input())\n",
    "for tc in range(1,T+1):\n",
    "    N = int(input())\n",
    "    sum = 0\n",
    "    for i in range(1,N+1):\n",
    "        if i%2 == 0:\n",
    "            sum-=i\n",
    "        else:\n",
    "            sum+=i\n",
    "    print('#{} {}'.format(tc,sum))"
   ]
